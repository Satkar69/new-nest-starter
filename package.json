    "class-validator": "^0.14.0",
    "exceljs": "^4.4.0",
    "express-rate-limit": "^7.0.1",
    "fast-csv": "^5.0.2",
    "ioredis": "^5.4.2",
    "joi": "^17.10.2",
    "nestjs-cls": "^3.5.1",
    "pg": "^8.11.3",
    "pg-query-stream": "^4.8.1",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "sharp": "^0.33.5",
    "slugify": "^1.6.6",
    "typeorm": "^0.3.17"
